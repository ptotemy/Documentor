
<%= content_for :page_header do %>
<span style="color:white"><%  "Dockets" %></span>
<% end %>

<table>
  <tr>
    <th>Name</th>
  </tr>
  <% for docket in @dockets %>
    <tr>
      <td><%= docket.name %></td>
      <td><%= link_to "Show", docket %></td>
      <td><%= link_to "Edit", edit_docket_path(docket) %></td>
      <td><%= link_to "Destroy", docket, :confirm => 'Are you sure?', :method => :delete %></td>
    </tr>
  <% end %>
</table>

<p><%= link_to "New Docket", new_docket_path %></p>
